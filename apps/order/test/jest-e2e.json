{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
   "^@lib/(.*)$": "<rootDir>/../../../libs/$1",
    "^@app/(.*)$": "<rootDir>/../../../apps/$1"
  },
  "setupFiles": ["<rootDir>/setup-env.ts"]
}
