{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "forceConsistentCasingInFileNames": false,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
  "types": ["node", "jest"],
    "paths": {
      "@app/*": [
        "apps/*"
      ],
      "@lib/*": [
        "libs/*"
      ],
      "@lib/redis-client": [
        "libs/redis-client/src"
      ],
      "@lib/redis-client/*": [
        "libs/redis-client/src/*"
      ],
      "@lib/config": [
        "libs/config/src"
      ],
      "@lib/config/*": [
        "libs/config/src/*"
      ],
      "@lib/address": [
        "libs/address/src"
      ],
      "@lib/address/*": [
        "libs/address/src/*"
      ],
      "@app/databaseMigration/*": [
        "apps/database/migrations/*"
      ],
      "@lib/http": [
        "libs/http"
      ],
      "@lib/constants": [
        "libs/constants"
      ],
      "@lib/swagger": [
        "libs/swagger"
      ],
      "@lib/dtos": [
        "libs/dtos"
      ],
      "@lib/log": [
        "libs/logger/src"
      ],
      "@lib/log/*": [
        "libs/logger/src/*"
      ],
      "@lib/sentry": [
        "libs/sentry/src"
      ],
      "@lib/sentry/*": [
        "libs/sentry/src/*"
      ],
      "@lib/notifications": [
        "libs/notifications/src"
      ],
      "@lib/notifications/*": [
        "libs/notifications/src/*"
      ],
      "@lib/websocket-client": [
        "libs/websocket-client/src"
      ],
      "@lib/websocket-client/*": [
        "libs/websocket-client/src/*"
      ]
    }
  }
}