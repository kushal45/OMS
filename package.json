{
  "name": "oms",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npx nest build",
    "build:all": "npx nest build",
    "postbuild:all": "sh scripts/copy-envs-after-build.sh",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug=0.0.0.0:9229 --watch",
    "start:prod": "node dist/apps/order/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/oms/test/jest-e2e.json",
    "migration:run": "dotenv -e .env.db -- ts-node node_modules/typeorm/cli.js migration:run -d  ./apps/config/dataSource.ts",
    "migration:generate": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./apps/database/migrations/$npm_config_name",
    "migration:create": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js  migration:create ./apps/database/migrations/$npm_config_name",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -d  ./apps/config/dataSource.ts",
    "seed": "dotenv -e .env.db -- ts-node apps/database/seeders/seed.ts",
    "prepare:migrations": "ts-node --project tsconfig.json create-migrations-table.ts",
    "run:migrations:custom": "dotenv -e .env.db -- ts-node run-migration.js",
    "deep-clean-migrations": "dotenv -e .env.db -- node deep-clean-migrations.js"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@faker-js/faker": "^9.2.0",
    "@grpc/grpc-js": "^1.12.4",
    "@grpc/proto-loader": "^0.7.13",
    "@kafkajs/confluent-schema-registry": "^3.3.0",
    "@nestjs/axios": "^3.1.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/elasticsearch": "^11.1.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.13",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "http-proxy-middleware": "^3.0.3",
    "ioredis": "^5.6.1",
    "kafkajs": "^2.2.4",
    "nest-winston": "^1.9.7",
    "nimma": "^0.7.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.0",
    "prom-client": "^15.1.3",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "save": "^2.9.0",
    "swagger-ui-express": "^5.0.1",
    "tsc": "^2.0.4",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.3",
    "uuidv4": "^6.2.13",
    "winston": "^3.17.0",
    "winston-elasticsearch": "^0.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/redlock": "^4.0.7",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "ts-proto": "^2.7.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/address(|/.*)$": "<rootDir>/libs/address/src/$1",
      "^@app/log(|/.*)$": "<rootDir>/libs/log/src/$1"
    }
  }
}
